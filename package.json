{
  "name": "titlescore-cf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001 --strictPort",
    "build": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-05-22 --port 3000 --proxy 3001 --d1=CONTEST_DB -- pnpm dev",
    "pages:deploy": "pnpm run build && wrangler pages publish ./build",
    "db:gen": "drizzle-kit generate:sqlite",
    "db:diff": "wrangler d1 migrations list CONTEST_DB",
    "db:apply:local": "wrangler d1 migrations apply CONTEST_DB --local --experimentalBackend",
    "db:apply": "wrangler d1 migrations apply CONTEST_DB --experimentalBackend"
  },
  "dependencies": {
